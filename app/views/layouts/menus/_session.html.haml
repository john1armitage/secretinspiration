- unless current_user.id.blank?
  %li
    != link_to "Profile", edit_user_path(current_user.id), :class => "button radius"
  %li
    != link_to "Log Out", logout_path, :method => :delete, :class => "button radius no_ajaxify"
- else
  %li
    != link_to "Sign Up", signup_path, :class => "button radius"
  %li
    != link_to "Log In", login_path, :class => "button radius"
